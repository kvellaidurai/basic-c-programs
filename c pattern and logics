1 print the pattern

#include <stdio.h>
int main() {
   int n=3,a,c,d;
   a=n*2-1;
   for(int i=0;i<a;i++)
   {
       for(int j=0;j<a;j++)
       {
         c=abs(i-a/2);
         d=abs(j-a/2);
       if(c>d)
       {
       printf("%d",c+1);
           
       }
       else
       {
       printf("%d",d+1);
           
       }
           
       }
       printf("\n");
   }
   }
